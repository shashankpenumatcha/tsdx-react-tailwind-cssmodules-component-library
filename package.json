{
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "npm run build && tsdx watch",
    "build": "tsdx build && npm pack",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "build-tailwind": "NODE_ENV=production npx tailwindcss -o ./tailwind.css --minify"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "beroe-shared-components",
  "author": "shashank.penumatcha@beroe-inc.com",
  "module": "dist/tsdx-lib.esm.js",
  "size-limit": [
    {
      "path": "dist/tsdx-lib.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/tsdx-lib.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "autoprefixer": "^10.4.14",
    "husky": "^8.0.3",
    "parcel": "1.12.3",
    "postcss": "^8.4.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.63.4",
    "size-limit": "^8.2.4",
    "tailwindcss": "^3.3.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "beroe-shared-components": "file:beroe-shared-components-0.0.0.tgz",
    "react-app-polyfill": "^3.0.0"
  }
}
